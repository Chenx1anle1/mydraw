        <mxCell id="i1omybJzdtganZt4SpFg-10" value="&lt;div&gt;SELECT DISTINCT m.*&lt;/div&gt;&lt;div&gt;FROM menu AS m&lt;/div&gt;&lt;div&gt;JOIN menu_auth AS ma ON ma.menuId = m.id&lt;/div&gt;&lt;div&gt;JOIN role_menu AS rm ON rm.menuId = ma.menuId&lt;/div&gt;&lt;div&gt;JOIN role_auth AS ra ON ra.roleId = rm.roleId&lt;/div&gt;&lt;div&gt;JOIN per_dept AS pd ON pd.deptId = ra.deptId&lt;/div&gt;&lt;div&gt;WHERE pd.perId = 1&lt;/div&gt;&lt;div&gt;AND pd.daoId = 2&lt;/div&gt;&lt;div&gt;AND ma.appId = 2&lt;/div&gt;&lt;div&gt;AND ma.daoId = 2&lt;/div&gt;&lt;div&gt;AND m.app_id = 2&lt;/div&gt;&lt;div&gt;-- AND m.app_id = 1&lt;/div&gt;&lt;div&gt;-- GROUP BY m.id&lt;/div&gt;&lt;div&gt;-- ORDER BY m.parent_id asc,m.id asc0&lt;/div&gt;" style="rounded=0;whiteSpace=wrap;html=1;align=left;" vertex="1" parent="1">
