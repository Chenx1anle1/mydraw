        <mxCell id="_Ulxhs72BKAG6LtGEGzr-28" value="&lt;div&gt;&lt;font color=&quot;#0000ff&quot;&gt;SELECT DISTINCT m.*&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font color=&quot;#0000ff&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; FROM menu AS m&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font color=&quot;#0000ff&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; JOIN menu_auth AS ma ON ma.menuId = m.id&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font color=&quot;#0000ff&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; JOIN role_menu AS rm ON rm.menuId = ma.menuId&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font color=&quot;#0000ff&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; JOIN role_auth AS ra ON ra.roleId = rm.roleId&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font color=&quot;#0000ff&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; JOIN per_dept AS pd ON pd.deptId = ra.deptId&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font color=&quot;#0000ff&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; WHERE pd.perId = &#39;${perId}&#39;&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font color=&quot;#0000ff&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; AND pd.daoId = &#39;${this.ctx.tokenClaims.daoid}&#39;&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font color=&quot;#0000ff&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; AND ma.appId = &#39;${this.ctx.tokenClaims.appid}&#39;&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font color=&quot;#0000ff&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; AND ma.daoId = &#39;${this.ctx.tokenClaims.daoid}&#39;&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font color=&quot;#0000ff&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; AND m.app_id = &#39;${this.ctx.tokenClaims.appid}&#39;&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font color=&quot;#0000ff&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; -- AND m.app_id = 1&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font color=&quot;#0000ff&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; -- GROUP BY m.id&lt;/font&gt;&lt;/div&gt;&lt;div&gt;&lt;font color=&quot;#0000ff&quot;&gt;&amp;nbsp; &amp;nbsp; &amp;nbsp; -- ORDER BY m.parent_id asc,m.id asc&lt;/font&gt;&lt;/div&gt;" style="text;html=1;align=left;verticalAlign=middle;resizable=0;points=[];autosize=1;" vertex="1" parent="1">
